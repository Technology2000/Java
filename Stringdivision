public class Dividing {
     public static void main(String args[]) {
      String str = "ababab";  
       int n = 3;  
       if((str.length()%n)!=0){
           System.out.println("The given string cannot be divided into"+n+"equal parts");
       }
       else{
       int m=str.length()/n;
       for(int i=0;i<str.length();i=i+m){
       System.out.println(str.substring(i,i+m));    
       }
       }
    }
}
